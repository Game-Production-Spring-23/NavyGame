        "playerPortrait2": "/assets/images/characterSplashes/engineer/engiC1.png",
        "otherPortrait": "/assets/images/characterSplashes/crossBirb/cb1.png",
        "isPlayerTalking": false,
        "text": "2) Logical Analysis and Design Solution ",
